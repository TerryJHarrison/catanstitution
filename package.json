{
  "name": "catanstitution",
  "version": "1.0.0",
  "scripts": {
    "deploy:local": "truffle migrate --compile-all --network local --f 2",
    "deploy:local:init": "truffle migrate --compile-all --network local --f 1 --to 1",
    "deploy:qa": "truffle migrate --compile-all--network mumbai --f 2",
    "deploy:qa:init": "truffle migrate --compile-all --network mumbai --f 1 --to 1",
    "clean": "truffle networks --clean",
    "networks": "truffle networks",
    "compile": "truffle compile --all",
    "contract:size":  "truffle run contract-size"
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@openzeppelin/truffle-upgrades": "^1.7.1",
    "@truffle/hdwallet-provider": "^1.4.0",
    "ethers": "^5.3.0",
    "truffle-contract-size": "^2.0.1",
    "web3": "^1.3.6"
  }
}
